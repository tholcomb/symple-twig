{
    "name": "tholcomb/symple-twig",
    "description": "Twig component for the Symple Framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tyler Holcomb",
            "email": "tyler@tholcomb.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tholcomb\\Symple\\Twig\\": ""
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "tholcomb/symple-core": "^1.0",
        "symfony/twig-bridge": "^5.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "tholcomb/symple-http": "^1.0",
        "phpunit/phpunit": "^8.5 | ^9.5"
    },
    "suggest": {
        "tholcomb/symple-console": "For template cache management"
    }
}
